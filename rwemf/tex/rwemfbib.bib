@article{he_neural_2017,
	title = {Neural Collaborative Filtering},
	url = {http://arxiv.org/abs/1708.05031},
	abstract = {In recent years, deep neural networks have yielded immense success on speech recognition, computer vision and natural language processing. However, the exploration of deep neural networks on recommender systems has received relatively less scrutiny. In this work, we strive to develop techniques based on neural networks to tackle the key problem in recommendation -- collaborative filtering -- on the basis of implicit feedback. Although some recent work has employed deep learning for recommendation, they primarily used it to model auxiliary information, such as textual descriptions of items and acoustic features of musics. When it comes to model the key factor in collaborative filtering -- the interaction between user and item features, they still resorted to matrix factorization and applied an inner product on the latent features of users and items. By replacing the inner product with a neural architecture that can learn an arbitrary function from data, we present a general framework named {NCF}, short for Neural network-based Collaborative Filtering. {NCF} is generic and can express and generalize matrix factorization under its framework. To supercharge {NCF} modelling with non-linearities, we propose to leverage a multi-layer perceptron to learn the user-item interaction function. Extensive experiments on two real-world datasets show significant improvements of our proposed {NCF} framework over the state-of-the-art methods. Empirical evidence shows that using deeper layers of neural networks offers better recommendation performance.},
	journaltitle = {{arXiv}:1708.05031 [cs]},
	author = {He, Xiangnan and Liao, Lizi and Zhang, Hanwang and Nie, Liqiang and Hu, Xia and Chua, Tat-Seng},
	urldate = {2017-11-12},
	date = {2017-08-16},
	eprinttype = {arxiv},
	eprint = {1708.05031},
	keywords = {Computer Science - Information Retrieval},
	file = {arXiv\:1708.05031 PDF:/home/neo/Zotero/storage/IDRXBN25/He 等。 - 2017 - Neural Collaborative Filtering.pdf:application/pdf;arXiv.org Snapshot:/home/neo/Zotero/storage/9RU5BMUH/1708.html:text/html}
}

@article{agarwal_stochastic_2012,
	title = {Stochastic optimization and sparse statistical recovery: An optimal algorithm for high dimensions},
	url = {http://arxiv.org/abs/1207.4421},
	shorttitle = {Stochastic optimization and sparse statistical recovery},
	abstract = {We develop and analyze stochastic optimization algorithms for problems in which the expected loss is strongly convex, and the optimum is (approximately) sparse. Previous approaches are able to exploit only one of these two structures, yielding an \${\textbackslash}order({\textbackslash}pdim/T)\$ convergence rate for strongly convex objectives in \${\textbackslash}pdim\$ dimensions, and an \${\textbackslash}order({\textbackslash}sqrt\{({\textbackslash}spindex {\textbackslash}log {\textbackslash}pdim)/T\})\$ convergence rate when the optimum is \${\textbackslash}spindex\$-sparse. Our algorithm is based on successively solving a series of \${\textbackslash}ell\_1\$-regularized optimization problems using Nesterov's dual averaging algorithm. We establish that the error of our solution after \$T\$ iterations is at most \${\textbackslash}order(({\textbackslash}spindex {\textbackslash}log{\textbackslash}pdim)/T)\$, with natural extensions to approximate sparsity. Our results apply to locally Lipschitz losses including the logistic, exponential, hinge and least-squares losses. By recourse to statistical minimax results, we show that our convergence rates are optimal up to multiplicative constant factors. The effectiveness of our approach is also confirmed in numerical simulations, in which we compare to several baselines on a least-squares regression problem.},
	journaltitle = {{arXiv}:1207.4421 [cs, math, stat]},
	author = {Agarwal, Alekh and Negahban, Sahand and Wainwright, Martin J.},
	urldate = {2017-11-15},
	date = {2012-07-18},
	eprinttype = {arxiv},
	eprint = {1207.4421},
	keywords = {Computer Science - Learning, Mathematics - Optimization and Control, Statistics - Machine Learning},
	file = {arXiv\:1207.4421 PDF:/home/neo/Zotero/storage/SAZUJMID/Agarwal 等。 - 2012 - Stochastic optimization and sparse statistical rec.pdf:application/pdf;arXiv.org Snapshot:/home/neo/Zotero/storage/8BEGLQYA/1207.html:text/html}
}

@article{choi_statistical_2015,
