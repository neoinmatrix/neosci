	keywords = {collaborative filtering, Internet, Prediction algorithms, quality of service, quality-of-service, Time factors, Web service recommendation, Web services, Vectors, data sparsity, Context, hybrid personalized random walk, hybrid personalized random walk algorithm, Inference algorithms, neighborhood-based collaborative filtering, {QoS} prediction methods, time information, time-aware approach, Web service recommendation technologies}
}

